{
  "name": "Chrome resize",
  "short_name": "Resize",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Simple browser resize",
  "icons": {
    "16": "images/default.png"
  },
  "browser_action": {
    "default_icon": "images/default.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}